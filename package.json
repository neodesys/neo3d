{
  "name": "neo3d",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "author": "Copyright (C) 2015, Lo√Øc Le Page",
  "private": true,
  "repository": "github:neodesys/neo3d",
  "devDependencies": {
    "file-loader": "6.1.1",
    "html-webpack-plugin": "4.5.0",
    "jshint": "2.12.0",
    "jshint-stylish": "2.2.1",
    "qunit": "2.9.3",
    "remove-comments-loader": "0.1.2",
    "rimraf": "3.0.2",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-glsl-loader": "1.0.1"
  },
  "scripts": {
    "jshint": "jshint --reporter=node_modules/jshint-stylish src test",
    "build": "rimraf dist && webpack --bail --progress",
    "dev": "webpack-dev-server --open",
    "test": "qunit test/*",
    "make": "npm run jshint && npm test && npm run build"
  },
  "jshintConfig": {
    "browser": true,
    "curly": true,
    "eqeqeq": true,
    "esversion": 5,
    "nonbsp": true,
    "nonew": true,
    "strict": "global",
    "undef": true,
    "unused": true,
    "predef": [
      "module",
      "require",
      "ImageData",
      "QUnit"
    ]
  }
}
